{
    "presets": [
        "es2015",
        "stage-0",
    ],
    "plugins": [
        ["lodash"],
        [
            "transform-runtime",
            {
                "helpers": false,
                "polyfill": false,
                "regenerator": true,
                "moduleName": "babel-runtime"
            }
        ],
	],
    "env": {
        "development": {
            "sourceMaps" : "inline",
        }
    }
}
